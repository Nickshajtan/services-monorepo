{
  "defaults": {
    "provider": "openai",
    "model": "gpt-4.1-mini",
    "temperature": 0.2
  },
  "providers": {
    "openai": {
      "apiKeyEnv": "OPENAI_API_KEY",
      "baseUrl": null,
      "timeoutMs": 30000
    }
  },
  "domains": {
    "devdocs": {
      "tasks": {
        "query": {
          "provider": "openai",
          "model": "gpt-4.1",
          "temperature": 0.2,
          "instructions": "You are a concise engineering assistant. Return markdown when useful."
        }
      }
    },
    "templates": {
      "module-readme": {
        "provider": "openai",
        "instructions": "You write repository documentation. Output only markdown.",
        "prompt": "Generate README for {{name}}.\n\nContext:\n{{context}}\n\nAPI:\n{{api}}\n"
      }
    },
    "branding": {
      "image": {
        "provider": "openai",
        "model": "gpt-image-1",
        "size": "1024x1024",
        "background": "transparent",
        "outputFormat": "png"
      }
    }
  }
}
